prefix=@CMAKE_INSTALL_PREFIX@
includedir=@PKGCONFIG_INCLUDE_DIR@
libdir=@PKGCONFIG_LIBRARY_DIR@

Name: libVROOM
Description: Project VROOM library
Version: @VROOM_VERSION@
Requires:
Libs: -L${libdir} -lvroom @PKGCONFIG_VROOM_LDFLAGS@
